{
  "name": "breeze-odata4",
  "version": "0.9.14",
  "description": "Library to allow OData 4 support for breezejs",
  "main": "./lib/index.js",
  "typings": "./lib/index",
  "files": [
    "/lib",
    "/typings"
  ],
  "scripts": {
    "build": "tsc && tsc -m es6 --outDir lib-esm",
    "build:prod": "webpack --config webpack.prod.ts",
    "prepublish": "npm run test&& npm run build:prod",
    "pretest": "clear",
    "start": "npm test -- --watch",
    "test": "jest",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "lint": "tslint --project tsconfig.json **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tschettler/breeze-odata4.git"
  },
  "keywords": [
    "breeze",
    "breezejs",
    "breeze-client",
    "odata",
    "odata4",
    "typescript"
  ],
  "author": "Travis Schettler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tschettler/breeze-odata4/issues"
  },
  "homepage": "https://github.com/tschettler/breeze-odata4#readme",
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "dependencies": {
    "breeze-client": "^1.7.0",
    "ts-odatajs": "^4.0.4"
  },
  "peerDependencies": {
    "breeze-client": "^1.7.0",
    "ts-odatajs": "^4.0.4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.5",
    "@types/webpack": "^4.4.18",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
